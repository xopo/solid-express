        "express-session": "^1.18.1",
        "knex": "^3.1.0",
        "node-cron": "^3.0.3",
        "nodemon": "^3.1.9",
        "optional": "^0.1.4",
        "sass": "^1.84.0",
        "sharp": "^0.33.5",
